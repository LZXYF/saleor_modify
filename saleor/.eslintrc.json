{
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": ["standard", "plugin:react/recommended"],
  "globals": {
    "Materialize": true,
    "describe": true,
    "gettext": true,
    "it": true,
    "ngettext": true,
    "pgettext": true
  },
  "plugins": ["react"],
  "parser": "babel-eslint",
  "rules": {
    "semi": ["error", "always"],
    "space-before-function-paren": "off"
  }
}
